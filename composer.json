{
  "name": "bepsvpt/ccu-plus",
  "description": "CCU PLUS API.",
  "keywords": ["ccu-plus"],
  "license": "AGPL",
  "type": "project",
  "require": {
    "php": "^7.2",
    "laravel/lumen-framework": "5.7.*",
    "laravel/tinker": "1.0.*",
    "vlucas/phpdotenv": "2.5.*"
  },
  "require-dev": {
    "fzaninotto/faker": "1.8.*",
    "phpunit/phpunit": "7.4.*",
    "mockery/mockery": "1.2.*"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/",
      "database/"
    ]
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"copy('.env.example', '.env');\""
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
