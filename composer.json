{
  "name": "bepsvpt/ccu-plus",
  "description": "CCU PLUS API.",
  "keywords": ["ccu-plus"],
  "license": "AGPL",
  "type": "project",
  "require": {
    "php": "^7.2",
    "algolia/algoliasearch-client-php": "1.27.*",
    "bepsvpt/secure-headers": "5.3.*",
    "doctrine/dbal": "2.9.*",
    "gregwar/captcha": "1.1.*",
    "guzzlehttp/guzzle": "6.3.*",
    "laravel/lumen-framework": "5.7.*",
    "laravel/scout": "6.1.*",
    "laravel/tinker": "1.0.*",
    "predis/predis": "1.1.*",
    "spatie/laravel-fractal": "5.4.*",
    "vlucas/phpdotenv": "2.5.*"
  },
  "require-dev": {
    "fzaninotto/faker": "1.8.*",
    "phpunit/phpunit": "7.5.*",
    "mockery/mockery": "1.2.*"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/",
      "database/"
    ]
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"copy('.env.example', '.env');\""
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
